#include <stdio.h>

int main() {
    int pages[] = {1, 3, 0, 3, 5, 6, 3};
    int n = 7, frame[3], fcount = 0, front = 0, pageFaults = 0;
    for(int i=0;i<3;i++) frame[i] = -1;

    for(int i=0;i<n;i++) {
        int found = 0;
        for(int j=0;j<3;j++)
            if(frame[j] == pages[i]) found = 1;
        if(!found) {
            frame[front] = pages[i];
            front = (front + 1) % 3;
            pageFaults++;
        }
        printf("Frames: ");
        for(int j=0;j<3;j++) printf("%d ", frame[j]);
        printf("\n");
    }
    printf("Total Page Faults = %d\n", pageFaults);
    return 0;
}


sample output:
Frames: 1 -1 -1
Frames: 1 3 -1
Frames: 1 3 0
Frames: 3 3 0
Frames: 3 5 0
Frames: 3 5 6
Frames: 3 5 6
Total Page Faults = 5

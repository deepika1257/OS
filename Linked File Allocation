#include <stdio.h>

int main() {
    int fileBlocks[] = {2, 5, 8, 10};
    int n = 4;
    printf("Linked File Allocation:\n");
    for(int i=0;i<n-1;i++)
        printf("Block %d -> %d\n", fileBlocks[i], fileBlocks[i+1]);
    printf("Block %d -> NULL\n", fileBlocks[n-1]);
    return 0;
}

sample output:
Linked File Allocation:
Block 2 -> 5
Block 5 -> 8
Block 8 -> 10
Block 10 -> NULL

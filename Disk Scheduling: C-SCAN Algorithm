#include <stdio.h>
#include <stdlib.h>

int main() {
    int req[] = {176, 79, 34, 60, 92, 11, 41, 114};
    int n = 8, head = 50, disk_size = 200;
    int seek = 0;

    int temp, i, j;
    for(i=0;i<n-1;i++)
        for(j=i+1;j<n;j++)
            if(req[i] > req[j]) { temp=req[i]; req[i]=req[j]; req[j]=temp; }

    int pos;
    for(i=0;i<n;i++) if(head < req[i]) { pos=i; break; }

    for(i=pos;i<n;i++) { seek += abs(req[i]-head); head=req[i]; }
    seek += abs((disk_size-1)-head);
    seek += (disk_size-1);
    head = 0;
    for(i=0;i<pos;i++) { seek += abs(req[i]-head); head=req[i]; }

    printf("Total Seek Time = %d\n", seek);
    return 0;
}


sample output:
Total Seek Time = 382
